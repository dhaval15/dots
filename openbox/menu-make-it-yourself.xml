        <action name="Execute">
          <command>pamac-updater</command>
        </action>
      </item>
      <separator/>
      <item label="File Manager As Root">
        <action name="Execute">
          <command>gksudo thunar</command>
        </action>
      </item>
      <item label="Text Editor As Root">
        <action name="Execute">
          <command>gksudo geany</command>
        </action>
      </item>
