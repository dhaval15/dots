source $HOME/Hive/dots/nvim/plugins.vim 

source $HOME/Hive/dots/nvim/plug-config/init.vim

source $HOME/Hive/dots/nvim/custom/init.vim

source $HOME/Hive/dots/nvim/snippets/init.vim

source $HOME/Hive/dots/nvim/flutter/init.vim

set termguicolors

let g:airline_theme = 'gruvbox_material'

let g:gruvbox_material_background = 'hard'

colorscheme gruvbox-material

set number

set mouse=a

set cursorline

set nocompatible
filetype plugin on
syntax on
